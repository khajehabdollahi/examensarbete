<% layout('layout/boilerplate') %>

<div class="row text-center text-capitalize">
    <div class="col-md-12">
    <h1>All Users</h1>
    <ul style="list-style-type:none ">
      <% for(let user of allUsers) { %>
        <li>
          <h4><a href="/users/<%= user.id %>"><%= user.name %></a></h4>
        </li> 
      <%} %>
   </ul>
  </div>
</div>